*&---------------------------------------------------------------------*
*&      Module  STATUS_0100  OUTPUT
*&---------------------------------------------------------------------*
MODULE STATUS_0100 OUTPUT.
  SET PF-STATUS '0100'.
  SET TITLEBAR '0100' WITH P_DDTEXT.

ENDMODULE.                 " STATUS_0100  OUTPUT
*&---------------------------------------------------------------------*
*&      Module  ALV_CL_PROCESS_1ST  OUTPUT
*&---------------------------------------------------------------------*
MODULE ALV_CL_PROCESS_1ST OUTPUT.
  PERFORM ALV_CL_PROCESS_1ST.
ENDMODULE.                 " ALV_CL_PROCESS_1ST  OUTPUT
